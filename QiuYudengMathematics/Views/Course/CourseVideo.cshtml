@model QiuYudengMathematics.Models.ViewModels.CourseManagementViewModel


<div id="app" class="course-vedio">
    <div style="margin:auto; width:80%">
        <h3>@Model.CourseName</h3>
        <iframe style="width: 100%; height:80vh" :src="'https://www.youtube.com/embed/'+para" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    </div>
</div>

<script>
    const parsingUrl = new URL("@Model.Url");
    const courseParam = parsingUrl.searchParams.get('v')
    const vm = new Vue({
        el:'#app',
        computed: {
            para() {
                return courseParam
            }
        }
    })
</script>