@model List<QiuYudengMathematics.Models.ViewModels.CourseManagementViewModel>
<div>影片待開發</div>

@if (Model.Count > 0)
{
    foreach (var item in Model)
    {
        <div>
            <span>@item.CourseName</span>
            <br />
            <iframe width="300" height="100" src="@item.Url" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
    }
}
else
{
    <div>查無課程</div>
}